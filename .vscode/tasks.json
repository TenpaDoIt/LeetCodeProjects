                "${workspaceRoot}/SourceFiles/IntegertoRoman.cpp",
                "${workspaceRoot}/SourceFiles/RomantoInteger.cpp",
                "${workspaceRoot}/SourceFiles/ThreeSum.cpp",
                "${workspaceRoot}/SourceFiles/LRUCache.cpp",
                "${workspaceRoot}/SourceFiles/LeetCodeMain.cpp",
                "-o",
                "LeetCodeMain.out"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
